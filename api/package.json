{
  "name": "web-app-boilerplate-api",
  "version": "1.0.0",
  "description": "web-app-boilerplate-api",
  "repository": "git@github.com:Vazerthon/web-app-boilerplate.git",
  "main": "dist/index.js",
  "author": "Paul Vallis <vallis1@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build-container": "docker build -t vallis/cv-playground .",
    "start-dev": "docker-compose kill && docker-compose build && docker-compose up",
    "build-app-dev": "babel src -d dist --no-comments --copy-files --ignore node_modules",
    "start-app-dev": "nodemon --watch src/ -e js --exec \"npm run build-app-dev && node --inspect=0.0.0.0:9229 .\"",
    "db": "knex"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "dotenv": "^6.0.0",
    "knex": "^0.15.2",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "nodemon": "^1.18.3",
    "objection": "^1.2.6",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "prettier-eslint": "^8.8.2"
  }
}
