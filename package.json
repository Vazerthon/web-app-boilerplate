{
  "name": "web-app-boilerplate",
  "version": "1.0.0",
  "description": "web-app-boilerplate",
  "repository": "git@github.com:Vazerthon/web-app-boilerplate.git",
  "author": "Paul Vallis <vallis1@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "install-api": "cd api && npm install",
    "start-api": "cd api && npm run start-dev",
    "install-web": "cd web && npm install",
    "start-web": "cd web && npm run start-dev",
    "start": "concurrently \"npm run start-api\" \"npm run start-web\""
  },
  "devDependencies": {
    "concurrently": "^5.0.2"
  }
}
